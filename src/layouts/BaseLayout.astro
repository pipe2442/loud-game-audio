---
const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="description" content="Astro description" />
    <meta name="viewport" content="width=device-width" />
    <link
      rel="icon"
      type="image/svg+xml"
      href="https://i.ibb.co/rmNb2qC/lga.png"
    />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
  </head>
  <body class="bg-black py-8 px-8 sm:px-16 md:px-24 lg:px-32">
    <header>
    </header>
    <main>
      <slot />
    </main>
  </body>
</html>
